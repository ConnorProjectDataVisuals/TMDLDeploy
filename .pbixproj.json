{
  "deployments": {
    "Datasets": {
      "mode": "Dataset",
      "source": {
        "type": "Folder",
        "path": "./dataset"
      },
      "authentication": {
        "type": "ServicePrincipal",
        "tenantId": "03997685-d3a0-4613-b48c-ba3cdc08f157",
        "clientId": "52c6844a-64de-41f8-a074-cc517a6b9429",
        "clientSecret": "%PBI_CLIENT_SECRET%"
      },
      "options": {        
        "dataset": {
          "replaceParameters": true,
          "deployEmbeddedReport": false
        },
        "refresh": {
          "enabled": true,
          "skipNewDataset": true,
          "method": "XMLA",
          "type": "Full",
          "tracing": {
            "enabled": true,
            "logEvents": {
              "filter": [
                "*|TabularRefresh|Partition",
                "*|ReadData|Partition"
              ]
            }
          }
        }
      },
      "parameters": {        
        "[Environment]": "{{ENVIRONMENT}}" 
      },
      "environments": {
        "Development": {
          "workspace": "DDD Reports",
          "displayName": "YOUR_DATASET_NAME",
          "refresh": {}
        },
        "Production": {
          "workspace": "TMDL {PRD}",
          "displayName": "Sales",
          "refresh": {}
        }
      }
    }
  }
}
